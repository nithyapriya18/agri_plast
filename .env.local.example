# Neon (required for API routes)
DATABASE_URL=postgresql://user:password@host/db?sslmode=require

# Google Sign-In (required for auth)
NEXT_PUBLIC_GOOGLE_CLIENT_ID=your-google-oauth-client-id.apps.googleusercontent.com

# Google Maps (required for map). In GCP Console enable: Maps JavaScript API, Places API, Geocoding API. Restrict key to HTTP referrers.
NEXT_PUBLIC_GOOGLE_MAPS_API_KEY=your-google-maps-api-key

# Leave empty to use same-origin Next.js API; set only when using standalone Express backend
# NEXT_PUBLIC_API_URL=http://localhost:3001

# AWS Bedrock (required for chat)
AWS_ACCESS_KEY_ID=your-aws-access-key-id
AWS_SECRET_ACCESS_KEY=your-aws-secret-access-key
AWS_REGION=us-east-2
# BEDROCK_MODEL_ID=global.anthropic.claude-haiku-4-5-20251001-v1:0

# Optional: Supabase (if you still use it for some flows)
# NEXT_PUBLIC_SUPABASE_URL=your-project-url.supabase.co
# NEXT_PUBLIC_SUPABASE_ANON_KEY=your-anon-key
